<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>codbr</id>
	<name>Codbr Module</name>
	<version>1.0</version>
	<package>org.openmrs.module.codbr</package>
	<author>Ali Raza</author>
	<description>
		Helps to start writing OpenMRS modules
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/codbr/update.rdf</updateURL>
	<require_version>1.9.3</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.codbr.codbrActivator</activator>
	
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		codbr.hbm.xml
				
	</mappingFiles>
	
	<require_modules>
        <require_module version="2.1.3">org.openmrs.module.htmlformentry</require_module>
         <require_module version="3.2">org.openmrs.module.uiframework</require_module>
		</require_modules> 

	<!-- Internationalization -->
	<!-- All message codes should start with codbr. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

